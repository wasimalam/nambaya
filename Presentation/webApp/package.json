{
  "name": "nambaya",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "build-dev": "npm run env -s && ng build --prod=false --progress=true",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@angular/service-worker": "~8.2.14",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@ng-bootstrap/ng-bootstrap": "^5.3.1",
    "@ngx-translate/core": "^11.0.1",
    "@swimlane/ngx-charts": "^10.1.0",
    "@types/file-saver": "^2.0.1",
    "@types/hammerjs": "^2.0.39",
    "angular-auth-oidc-client": "^10.0.15",
    "angular2-signaturepad": "^3.0.4",
    "bootstrap": "^4.6.0",
    "file-saver": "^2.0.5",
    "google-libphonenumber": "^3.2.19",
    "hammerjs": "^2.0.8",
    "igniteui-angular": "^8.2.31",
    "igniteui-angular-core": "8.2.12",
    "igniteui-cli": "^4.3.5",
    "insert-text-at-cursor": "^0.3.0",
    "intl-tel-input": "^17.0.3",
    "material-design-icons-iconfont": "^5.0.1",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.33",
    "ng-click-outside": "^6.0.0",
    "ng-pick-datetime": "^7.0.0",
    "ng-pick-datetime-moment": "^1.0.8",
    "ngx-avatar": "^3.7.0",
    "ngx-bootstrap": "^5.6.0",
    "ngx-cookie-service": "^2.4.0",
    "ngx-countdown": "^8.0.4",
    "ngx-file-drop": "^8.0.8",
    "ngx-intl-tel-input": "^2.6.0",
    "ngx-toastr": "^11.3.3",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.6.6",
    "tslib": "^1.14.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "~8.3.2",
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "8.2.14",
    "@angularclass/hmr": "^2.1.3",
    "@aspnet/signalr": "^1.1.4",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "^3.6.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/jest": "^24.9.1",
    "@types/jszip": "^3.4.1",
    "@types/node": "^10.17.55",
    "codelyzer": "^5.2.2",
    "hads": "^1.7.0",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.7.1",
    "jest-preset-angular": "^7.1.0",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "protractor": "^5.4.4",
    "stylelint": "~10.1.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended-scss": "~3.3.0",
    "stylelint-config-standard": "~18.3.0",
    "stylelint-scss": "~3.9.0",
    "ts-node": "^8.10.2",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.5.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
